{
    "name": "atlasforge",
    "version": "0.1.0",
    "private": true,
    "packageManager": "pnpm@10.27.0",
    "scripts": {
        "dev": "turbo run dev",
        "build": "turbo run build",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "dev:api": "pnpm --filter @atlasforge/api dev",
        "dev:storefront": "pnpm --filter @atlasforge/storefront dev",
        "dev:landing": "pnpm --filter @atlasforge/landing dev",
        "build:api": "pnpm --filter @atlasforge/api build",
        "build:storefront": "pnpm --filter @atlasforge/storefront build",
        "build:landing": "pnpm --filter @atlasforge/landing build"
    },
    "devDependencies": {
        "turbo": "^2.5.4"
    }
}